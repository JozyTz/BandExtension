{
  "manifest_version": 2,
  "name": "Band Nickname Changer",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://band.us/*"],
      "js": ["content.js"]
    }
  ]
}
